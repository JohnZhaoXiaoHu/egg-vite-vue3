{
  "name": "egg-vite-vue3",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "eggScriptsConfig": {
    "daemon": true,
    "env": "prod",
    "title": "egg-vite-vue3"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "egg-scripts start ./output",
    "stop": "egg-scripts stop",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "build": "vite build -c vite.config.ts && npm run tsc",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.prod.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "deploy": "sh ./scripts/build.sh"
  },
  "dependencies": {
    "@forsigner/egg-typeorm": "^1.5.1",
    "egg": "^2.29.3",
    "egg-cors": "^2.2.3",
    "egg-decorator-router": "^1.0.7",
    "egg-jwt": "^3.1.7",
    "egg-passport": "^2.1.1",
    "egg-scripts": "^2.13.0",
    "egg-sequelize": "^6.0.0",
    "egg-view-nunjucks": "^2.3.0",
    "egg-vite-plugin": "^1.1.4",
    "mysql2": "^2.2.5",
    "passport-local": "^1.0.0",
    "typeorm": "^0.2.31",
    "umzug": "^2.3.0"
  },
  "devDependencies": {
    "@babel/standalone": "7.13.14",
    "@json2render/vue-full": "^0.1.10",
    "@types/egg": "^1.5.0",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash-es": "^4.17.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.35",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.0",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "3.0.11",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "egg-bin": "^4.15.0",
    "egg-ci": "^1.18.0",
    "egg-mock": "^4.0.1",
    "element-plus": "^1.0.2-beta.36",
    "eslint": "^7.22.0",
    "eslint-config-egg": "^9.0.0",
    "js-cookie": "^2.2.1",
    "lerna": "^4.0.0",
    "lodash-es": "^4.17.21",
    "mkdirp": "^1.0.4",
    "mocha": "5.2.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "postcss": "^8.2.8",
    "sass": "^1.32.8",
    "screenfull": "^5.1.0",
    "sequelize-cli": "^6.2.0",
    "tailwindcss": "^2.0.4",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "vite": "^2.1.4",
    "vue": "^3.0.11",
    "vue-router": "^4.0.5"
  },
  "repository": {
    "type": "git",
